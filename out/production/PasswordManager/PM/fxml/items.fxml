      <ImageView fx:id="ivicon" fitHeight="40.0" fitWidth="40.0" layoutX="14.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/icons8-facebook-48.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
